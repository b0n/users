{
    "name": "funayaki/users",
    "description": "Users plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/funayaki/acl-manager"
        }
    ],
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Users\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Users\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
